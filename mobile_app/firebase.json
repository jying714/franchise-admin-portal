{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ],
  "flutter": {
    "platforms": {
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "doughboyspizzeria-2b3d2",
          "configurations": {
            "android": "1:739021537990:android:43d49458c788c07ffe0dc7",
            "ios": "1:739021537990:ios:bd55027d8875c8befe0dc7"
          }
        }
      },
      "android": {
        "default": {
          "projectId": "doughboyspizzeria-2b3d2",
          "appId": "1:739021537990:android:43d49458c788c07ffe0dc7",
          "fileOutput": "android/app/google-services.json"
        }
      }
    }
  }
}
